<?xml version="1.0" encoding="utf-8"?>
<androidx.preference.PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto">

    <PreferenceCategory
            android:layout="@layout/preferences_title"
            app:key="task_settings"
            app:title="Settings for Delayed Nonmatch To Sample task">

        <mymou.preferences.SeekBarPreferenceCustom
                android:defaultValue="@integer/default_dnts_duration_on"
                app:key="@string/preftag_dnts_duration_on"
                app:title="Duration (ms) to show the cue" />

        <mymou.preferences.SeekBarPreferenceCustom
                android:defaultValue="@integer/default_dnts_startDelay"
                app:key="@string/preftag_dnts_startDelay"
                app:title="Wait time (ms) until cue" />

        <mymou.preferences.SeekBarPreferenceCustom
                android:defaultValue="@integer/default_od_duration_off"
                app:key="@string/preftag_dnts_duration_off"
                app:title="Wait time (ms) until choice" />


    </PreferenceCategory>

</androidx.preference.PreferenceScreen>